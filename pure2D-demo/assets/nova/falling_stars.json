{
	"version": 1,
	"emitters": [
		{
			"name": "falling_stars",
			"lifespan": 5000,
			"particles": [
				{
					"sprite": ["star"],
					"start_frame": [0, 20],
					"animator": ["fall_ani", "shoot_ani"],
					"blend_mode": ["", "screen", "add", "subtract"]
				}
			]
		}
	],
	
	"animators": [
		{
			"name": "fall_ani",
			"type": "recursive_trajectory",
			"velocity": [50, 100],
			"angle": [0, 6.28]
		},

		{
			"name": "shoot_ani",
			"type": "translate",
			"dx": [-500, 500],
			"dy": [-500, 500],
			"duration": [1000],
			"interpolation": "decelerate"
		}
	]
}
